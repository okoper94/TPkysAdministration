        <activity
            android:name=".activities.activity.MyPageActivity"
            android:exported="false" />
        <activity
            android:name=".activities.activity.EmailSignInActivity"
            android:exported="false" />
        <activity
            android:name=".activities.activity.MembershipActivity"
            android:exported="false" />
        <activity
            android:name=".MembershipActivity"
            android:exported="false" />
        <activity
            android:name=".activities.activity.Signin_MembershipActivity"
            android:exported="false" />
        <activity
            android:name=".activities.activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activities.activity.IntroActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.activity.MainActivity"
            android:exported="false" />
    </application>

</manifest>